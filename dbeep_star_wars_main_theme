lnote 8  ${E[3]}
lnote 8  ${C[4]}
lnote 8  ${B[3]}
lnote 8  ${A[3]}
lnote 8  ${G[3]}

ldnote 8  ${D[4]}
lnote 16 ${A[3]}
lnote 2  ${A[3]}
