tnote 8  ${F[4]}
ltnote 4  ${E[4]}
tnote 8  ${D[4]}
ltnote 4  ${C[4]}
tnote 8  ${Bb[3]}
ltnote 4  ${A[3]}
tnote 8  ${G[3]}

# this measure is a bit odd on the score, I simplified it a bit.
lnote 4  ${D[4]}
 note    ${A[3]} $((${L[2]} * 8 / 6)) $((${L[2]} * 1 / 6 + ${L[8]})) # extra pause
tnote 8  ${D[3]}
tnote 8  ${D[3]}
tnote 8  ${D[3]}

