lnote 2  ${D[4]}

tnote 8  ${C[4]}
tnote 8  ${B[3]}
tnote 8  ${A[3]}
lnote 2  ${G[4]}
lnote 4  ${D[4]}

tnote 8  ${C[4]}
tnote 8  ${B[3]}
tnote 8  ${A[3]}
lnote 2  ${G[4]}
lnote 4  ${D[4]}

tnote 8  ${C[4]}
tnote 8  ${B[3]}
tnote 8  ${C[4]}
ldnote 2  ${A[3]} # a bit longer than on score

# leave out the strange ending of the score

dbeep
