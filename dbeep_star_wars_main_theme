tnote 8  ${D[3]}
tnote 8  ${D[3]}
tnote 8  ${D[3]}

lnote 2  ${G[3]}
lnote 2  ${D[4]}

tnote 8  ${C[4]}
tnote 8  ${B[3]}
tnote 8  ${A[3]}
